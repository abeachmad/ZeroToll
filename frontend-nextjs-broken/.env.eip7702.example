# EIP-7702 Configuration for ZeroToll Frontend
# Copy this to .env.local and fill in your values

# Pimlico API Key (get from https://dashboard.pimlico.io/)
# Free tier: 1,000 UserOps/month
REACT_APP_PIMLICO_API_KEY=your_pimlico_api_key_here

# Backend URL (default for local development)
REACT_APP_BACKEND_URL=http://localhost:8000

# Optional: Enable debug logging
REACT_APP_DEBUG_EIP7702=true
